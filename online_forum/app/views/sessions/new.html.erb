<div class="container">
<p id="alert"><%= alert %></p>
<h1 class="title">Login</h1>
<%= form_tag sessions_path do |form| %>
  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="actions">
    <%= submit_tag "Login", class: 'button is-primary' %>
  </div>
<% end %>
<%= link_to 'Signup', signup_path %>
</div>